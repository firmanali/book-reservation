{{template "base" .}}
{{define "content"}}
    <div class="container">
        <div class="row">
            <div class="col">
                <h1>This is about page</h1>
                <p>This is from template: {{index .StringMap "test"}}</p>
                <p>{{if ne (index .StringMap "remote_ip") ""}}
                    Your remote IP is {{index .StringMap "remote_ip"}}
                    {{else}}
                    Unknown remote IP. Please visit <a href="/">home page</a> first!
                    {{end}}
                </p>
            </div>
        </div>
    </div>
{{end}}